<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD myth.javaConfig.Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="useGeneratedKeys" value="true" />
        <setting name="useColumnLabel" value="true" />
        <setting name="mapUnderscoreToCamelCase" value="true" />
    </settings>
    <typeHandlers>
        <typeHandler
                javaType="com.myth.type.Enabled"
                handler="com.myth.type.EnabledTypeHandler"/>
    </typeHandlers>
    <mappers>
        <package name="com.myth.mapper"/>
    </mappers>
</configuration>